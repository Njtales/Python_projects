    }
   },
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "('and', 25)\n",
      "('of', 14)\n",
      "('the', 11)\n",
      "('computer', 7)\n",
      "('to', 6)\n"
     ]
    }
   ],
   "source": [
    "words = []\n",
    "\n",
    "with open(\"demo_text.txt\", \"r\") as f:\n",
    "    for line in f:\n",
    "        words.extend(line.split())\n",
    "\n",
    "from collections import Counter\n",
    "\n",
    "counts = Counter(words)\n",
    "\n",
    "top5 = counts.most_common(5)\n",
    "for i in top5:\n",
    "    print(i)"
   ]
  }
